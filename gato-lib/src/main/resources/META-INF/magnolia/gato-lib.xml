<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>gato-lib</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <class>edu.txstate.its.gato.GatoLib</class>
  <versionHandler>edu.txstate.its.gato.setup.GatoLibVersionHandler</versionHandler>
  <version>${project.version}</version>
	<components>
		<id>main</id>
		<component>
			<type>edu.txstate.its.gato.GatoUtils</type>
			<implementation>edu.txstate.its.gato.GatoUtils</implementation>
			<scope>singleton</scope>
		</component>
    <component>
      <type>edu.txstate.its.gato.GatoMIMEMapping</type>
      <implementation>edu.txstate.its.gato.GatoMIMEMapping</implementation>
      <scope>singleton</scope>
    </component>
		<component>
			<type>edu.txstate.its.gato.ImagingModuleResizer</type>
			<implementation>edu.txstate.its.gato.ImagingModuleResizer</implementation>
		</component>
		<component>
			<type>edu.txstate.its.gato.GatoResizer</type>
			<implementation>edu.txstate.its.gato.GatoResizer</implementation>
		</component>
	</components>
  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>rendering</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>site</name>
      <optional>true</optional>
    </dependency>
  </dependencies>
</module>
